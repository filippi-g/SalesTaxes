<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>EntryPoint</servlet-name>
		<servlet-class>com.company.controller.EntryPoint</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ShowBasket</servlet-name>
		<servlet-class>com.company.controller.ShowBasket</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ShowReceipt</servlet-name>
		<servlet-class>com.company.controller.ShowReceipt</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>EntryPoint</servlet-name>
		<url-pattern>/EntryPoint</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ShowBasket</servlet-name>
		<url-pattern>/ShowBasket</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ShowReceipt</servlet-name>
		<url-pattern>/ShowReceipt</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>EntryPoint</welcome-file>
	</welcome-file-list>

</web-app>


